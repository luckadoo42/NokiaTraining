                <font weight="bold"><font-data afm="CheltenhamCdITC-Bold.afm" pfb="CheltenhamCdITC-Bold.pfb"/></font>
		<font weight="normal" style="italic"><font-data afm="CheltenhamCdITC-LightItalic.afm" pfb="CheltenhamCdITC-LightItalic.pfb"/></font>
                <font weight="bold" style="italic"><font-data afm="CheltenhamCdITC-BoldItalic.afm" pfb="CheltenhamCdITC-BoldItalic.pfb"/></font>
	
		<font-alias name="Cheltenham" value="CheltenhamCdITC-Light"/>
		<font-alias name="CheltenhamCdITC" value="CheltenhamCdITC-Light"/>
	  </font-family>
	  
	  <font-family name="Univers"> 
		<font><font-data
			afm="LT_50138.AFM"
			pfb="LT_50138.PFB"/></font>
		<font-alias name="Univers-Medium" value="Univers"/>
	  </font-family>

	  <font-family 
		name="Clarendon"> 
		<font><font-data
			afm="LT_50243.afm"
			pfb="LT_50243.pfb"/></font>
	  </font-family>

	  <font-family name="Trebuchet">
		<font><font-data ttf="trebuc.ttf"/></font>
        <font style="oblique"><font-data ttf="trebucit.ttf"/></font>
        <font weight="bold"><font-data ttf="trebucbd.ttf"/></font>
        <font weight="bold" style="oblique"><font-data ttf="trebucbi.ttf"/></font>
	  </font-family>


	</font-group>

