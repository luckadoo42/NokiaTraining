<?xml version='1.0' encoding='utf-8'?>
<l10n xmlns:l="http://docbook.sourceforge.net/xmlns/l10n/1.0" language="nl">

  <l:gentext key="Chapter" text=""/>
  <l:gentext key="chapter" text=""/>

  <l:gentext key="page.citation" text=""/>

  <l:gentext key="Published" text=""/>
  <l:gentext key="published" text=""/>

  <l:gentext key="RelatedTopics" text="Related Topics"/>

  <l:context name="title">
	<l:template name="appendix" text="%t"/>
	<l:template name="chapter" text="%t"/>
	<l:template name="equation" text="%t"/>
	<l:template name="example" text="%t"/>
	<l:template name="figure" text="%t"/>
	<l:template name="table" text="%t"/>
	<!--l:template name="glossary" text=""/-->
	<l:template name="procedure" text="%t"/>
	<!--l:template name="table" text="Table %n %t"/-->
  </l:context>

  <l:context name="section-title-numbered">
	<l:template name="sect1" text="%t"/>
	<l:template name="sect2" text="%t"/>
	<l:template name="sect3" text="%t"/>
	<l:template name="sect4" text="%t"/>
	<l:template name="sect5" text="%t"/>
	<l:template name="section" text="%t"/>
  </l:context>

  <l:context name="xref">

	<l:template name="appendix" text="%t"/>

	<l:template name="chapter" text="%t"/>

	<l:template name="part" text="%t"/>
	<l:template name="example" text="%t"/>
	<l:template name="figure" text="%t"/>
	<l:template name="table" text="%t"/>
	<!-- Changed from text="Procedure %n" -->
	<l:template name="procedure" text="%t"/>
	<!-- Added --><!-- smartquote to &# 8220; dumbquote: &# x0022; -->
	<l:template name="section" text="%t"/>

  </l:context>

  <l:context name="xref-number-and-title">
	
	<l:template name="appendix" text="%t"/>

	<l:template name="chapter" text="%t"/>
	
	<l:template name="part" text="%t"/>
	<l:template name="example" text="%t"/>
	<l:template name="figure" text="%t"/>
	<l:template name="table" text="%t"/>
	<!-- Changed from text="Procedure %n" -->
	<l:template name="procedure" text="%t"/>
	<!-- Added --><!-- smartquote to &# 8220; dumbquote: &# x0022; -->
	<l:template name="section" text="%t"/>
	
  </l:context>

  <l:context name="section-xref">
	<l:template name="section" text="%t"/>
	<l:template name="simplesect" text="%t"/>
  </l:context>

  <l:context name="section-xref-numbered">
	<l:template name="section" text="%t"/>
	<l:template name="simplesect" text="%t"/>
  </l:context>

</l10n>
