                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" id="unique_54__unique_54_Connect_42_table_lf4_dfp_plb__entry__1"><fo:block>Field</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" id="unique_54__unique_54_Connect_42_table_lf4_dfp_plb__entry__2"><fo:block>Description</fo:block></fo:table-cell>
                                                </fo:table-row>
                                            </fo:table-header><fo:table-body>
                                                <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>name</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>The name of the filter
                                                  action</fo:block></fo:table-cell>
                                                </fo:table-row>
                                                <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>context</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>The name of the context
                                                  in which the action resides</fo:block></fo:table-cell>
                                                </fo:table-row>
                                                <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Parameter Name and
                                                  Value</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>
                                                  <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">For
                                                  parameterized actions, the parameter names and
                                                  values</fo:block></fo:block-container>
                                                  <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Optional</fo:block></fo:block-container>
                                                  </fo:block></fo:table-cell>
                                                </fo:table-row>
                                            </fo:table-body></fo:table></fo:block-container>
                                    </fo:block></fo:block-container>
                                </fo:block></fo:block-container>
                            </fo:block>
                            <fo:block role="Sect" id="unique_55" chfox:nd-id="rule_dictionaries_definitions_sm_me">
                                <fo:block-container font-size="1.7em" space-after="0.5em" margin-left="0" margin-right="0" space-before="1.5em" page-break-after="avoid" id="ariaid-title55"><fo:block end-indent="0" start-indent="0" role="H2">Rule
                                    dictionaries and definitions across SM and MEs</fo:block></fo:block-container>
                                
                                <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                    <fo:block-container space-after="1em" space-before="1em" id="unique_55_Connect_42_section_spx_jfp_plb" chfox:nd-id="section_spx_jfp_plb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                        <fo:block-container id="d34e41216" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H3">Overview</fo:block></fo:block-container>
                                        <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">The custom
                                            rule dictionary, along with the attribute definition,
                                            and custom vendor definition files must be copied to all
                                            SM and/or ME applications. This includes custom rule
                                            dictionaries for Charging, Policy, and SM.</fo:block></fo:block-container>
                                        <fo:block>You must
                                            perform both of the following procedures, one after the
                                                other:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="1.33em" space-before="1.33em" id="unique_55_Connect_42_ul_l2w_mfp_plb" chfox:nd-id="ul_l2w_mfp_plb">
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Installing the custom rule
                                                  dictionary</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>To install, copy, and
                                                  distribute the attribute definition and custom
                                                  vendor definition files</fo:block></fo:list-item-body></fo:list-item>
                                            </fo:list-block></fo:block>
                                        <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">The SPS also
                                            supports the use of an unlimited number of VSA
                                            dictionary files and the Attribute definition
                                            dictionaries. The VSA files can be used across all
                                            Diameter interfaces. Each dictionary defines the list of
                                            valid attributes that are supported by the vendor and
                                            are available for SPS-Policy and SPS-Charging
                                            management. The dictionaries must be on the SPS
                                            workstation to be loaded. You add a VSA to a vendor
                                            dictionary to add the VSA to the SPS. To display the VSA
                                            in the GUI or to use it in rules, you must also add the
                                            VSA to the VsaToContextMapping.xml file in the
                                            respective contexts.</fo:block></fo:block-container>
                                        <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">If you are
                                            adding VSAs, see "Adding VSAs".</fo:block></fo:block-container>
                                    </fo:block></fo:block-container>
                                    <fo:block-container space-after="1em" space-before="1em" id="unique_55_Connect_42_section_awf_qfp_plb" chfox:nd-id="section_awf_qfp_plb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                        <fo:block-container id="d34e41244" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H3">Adding
                                            VSAs</fo:block></fo:block-container>
                                        <fo:block>The workflow to
                                            create or modify a dictionary is as follows:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="1.33em" space-before="1.33em" id="unique_55_Connect_42_ul_ark_rfp_plb" chfox:nd-id="ul_ark_rfp_plb">
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>create an entry in the
                                                  CustomVendorDefinition.xml for the vendor</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>create a dictionary by name
                                                  and specify all of the VSAs in the
                                                  VsaToContextMapping.xml file</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>reload all of the VSA
                                                  dictionaries to load the changes</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>distribute the files to all
                                                  of the nodes</fo:block></fo:list-item-body></fo:list-item>
                                            </fo:list-block></fo:block>
                                        <fo:block>To add or
                                            modify attributes, you need to create a dictionary and
                                            add or modify the attributes (see To add to the
                                            attribute definition and custom vendor definition
                                            files) in the dictionary in the
                                            opt/tpa/conf/sac/diameter directory. You can add a VSA
                                            dictionary file to the SPS at any time. The format of
                                            the file
                                            is:<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>/opt/tpa/schemas/DiameterDefinition.xsd</fo:inline></fo:block></fo:block-container></fo:block>
                                        <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">To display
                                            the VSA in the SPS rule engine, the GUI, or to be able
                                            to use the VSA in rules for Policy or Charging, you must
                                            also add the VSA to the VsaToContextMapping.xml in the
                                            respective files and contexts. That is, a VSA entry must
                                            be present in a dictionary and a source and/or result
                                            context mapping must be in the VSA mapping file.</fo:block></fo:block-container>
                                        <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">To use a VSA
                                            on specific interfaces only, the VsaRuleSourceContext
                                            and VsaRuleResultContext must be mapped to the VSA. When
                                            the context is mapped to the VSA, the VSA is only
                                            available as a criteria or an action along with the
                                            configured contexts.</fo:block></fo:block-container>
                                        <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">To configure
                                            VSA entries in a dictionary, use the
                                            VsaToContextMapping.xml file. The
                                            VsaToContextMapping.xml file provides an xml document
                                            that can be modified to include the individual VSA
                                            definitions.</fo:block></fo:block-container>
                                        <fo:block>The
                                            VsaToContextMapping.xml file for Policy is located
                                            in:<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>/opt/tpa/conf/sac/diameter/rule/VsaToContextMapping.xml</fo:inline></fo:block></fo:block-container></fo:block>
                                        <fo:block>The
                                            VsaToContextMapping.xml file for Charging is located
                                            in:<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>/opt/tpa/conf/sac/diameter/rule/ChargingVsaToContextMapping.xml</fo:inline></fo:block></fo:block-container></fo:block>
                                        <fo:block>Copy the
                                            contents of the VSA mapping file and enter the required
                                            values for the VSAs you are adding, as
                                            follows:<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>&lt;diameterToRuleContextMap xmlns="diameterRuleContextMap"&gt;
&lt;vsaToContext&gt;
&lt;vsaToRuleSourceContexts&gt;
&lt;vsaRuleSourceContext&gt;
&lt;context type="CONTEXT"/&gt;
&lt;vendorInfo&gt;
&lt;name&gt;vendorname&lt;/name&gt;
&lt;includeAttribute name=attribute-VSA/&gt;
&lt;includeAttribute name=attribute-VSA2 maxOccurs="unbounded"
additive="true"/&gt;
&lt;includeAttribute name=attribute-VSA3 valueType=
"EnumValueType" valueTypeClassName="com.alcatel.tpapps.diameter.par.
EventTrigger"/&gt;
&lt;includeAttribute name=attribute-VSA4.attribute-VSA5
includeGroupIndexArgs="true"/&gt;
&lt;/vendorInfo&gt;
&lt;/vsaRuleSourceContext&gt;
&lt;/vsaToRuleSourceContexts&gt;
&lt;vsaToRuleResultContexts&gt;
&lt;vsaRuleResultContext&gt;
&lt;context type="CONTEXT"/&gt;
&lt;vendorInfo&gt;
&lt;name&gt;vendorname&lt;/name&gt;
&lt;includeAttribute name=attribute-VSA/&gt;
&lt;includeAttribute name=attribute-VSA2 maxOccurs="unbounded"
additive="true"/&gt;
&lt;includeAttribute name=attribute-VSA3 valueType=
"EnumValueType" valueTypeClassName="com.alcatel.tpapps.diameter.par.
EventTrigger"/&gt;
&lt;includeAttribute name=attribute-VSA4.attribute-VSA5
includeGroupIndexArgs="true"/&gt;
&lt;/vendorInfo&gt;
&lt;/vsaRuleResultContext&gt;
&lt;/vsaToRuleResultContext&gt;
&lt;/vsaToContext&gt;
&lt;/diameterToRuleContextMap&gt;</fo:inline></fo:block></fo:block-container></fo:block>
                                        <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Save and add
                                            the file back to the respective directory.</fo:block></fo:block-container>
                                        <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Perform To
                                            install, copy, and distribute the attribute definition
                                            and custom vendor definition files to copy and
                                            distribute the VSA mapping file, the attribute
                                            definition and custom vendor definition files.</fo:block></fo:block-container>
                                        <fo:block><fo:inline font-weight="bold">To install, copy, and
                                                distribute the custom rule attribute
                                                dictionary</fo:inline><fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="1.33em" space-before="1.33em" id="unique_55_Connect_42_ol_icl_ggp_plb" chfox:nd-id="ol_icl_ggp_plb">
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">1.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Obtain the
                                                  CustomRuleAttributeDictionary.xml files that you
                                                  need from the appropriate repository location
                                                  provided by Nokia (for example, the
                                                  PCRFCustomRuleAttributeDictionary.xml,
                                                  ChargingCustomRuleAttributeDictionary.xml,
                                                  diameter/rule/ChargingVsaToContextMapping.xml,
                                                  diameter/rule/VsaToContextMapping.xml or
                                                  RSCustomRuleAttributeDictionary.xml).</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">2.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Connect to the SM active
                                                  OAME as the root user.</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">3.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">Put the file obtained in
                                                  step 1 in a directory of your choice on the SMs
                                                  Active OAME.<fo:block-container space-after="1em" space-before="1em"><fo:block end-indent="0" start-indent="0" role="Note"><fo:inline background-image="url('file:/C:/Program%20Files/Oxygen%20XML%20Author%2021/frameworks/dita/DITA-OT3.x/plugins/com.oxygenxml.webhelp.responsive/oxygen-webhelp/app/img/note.svg')" background-position-horizontal="left" background-position-vertical="center" background-repeat="no-repeat" font-weight="bold" padding-bottom="4px" padding-left="23px" padding-right="4px" padding-top="4px">Note:</fo:inline> Before
                                                  copying any files to any SPS VMs, the user must
                                                  execute the df and du Linux commands from each VM
                                                  to determine the storage available and whether or
                                                  not the copying of new files can proceed. Usage of
                                                  the Linux commands df (abbreviation for disk free)
                                                  and du (abbreviated from disk usage) can be found
                                                  on every VM via the Linux manual page by using
                                                  man df and man du as root user. There are two
                                                  storage directories that can be used on any VM as
                                                  follows:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_55_Connect_42_ul_tv1_jgp_plb" chfox:nd-id="ul_tv1_jgp_plb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>First choice, is under the
                                                  /appdata directory which is created and mounted to
                                                  the cinder volume (OpenStack)/independent disk
                                                  (VMware) on every VM.</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Second choice, is under the
                                                  /var/log directory which is created and mounted to
                                                  the cinder volume (OpenStack)/independent disk
                                                  (VMware) on every VM. If there is enough storage
                                                  available, proceed. When you have completed the
                                                  task, clean-up the files that are no longer
                                                  needed. If there is not enough space, either do
                                                  some clean-up or contact Nokia Support.</fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block></fo:block></fo:block-container></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">4.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">If you are installing a new
                                                  PCRFCustomRuleAttributeDictionary.xml file, a new
                                                  ChargingCustomRuleAttributeDictionary.xml,
                                                  diameter/rule/ ChargingVsaToContextMapping.xml,
                                                  diameter/rule/VsaToContextMapping.xml or a new
                                                  RSCustomRuleAttributeDictionary.xml run the
                                                  following command on the SMs Active
                                                  OAME:<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>/opt/tpa/bin/distribute-rules-dictionaries -d &lt;directory&gt; [-e &lt;MEs&gt;]
[-i &lt;MEs&gt;]</fo:inline></fo:block></fo:block-container>Where:<fo:block-container><fo:block font-weight="bold" space-before="0.5em" page-break-after="avoid" text-align="left"/><fo:table relative-align="baseline" chfox:html-table-width-policy="yes" table-layout="auto" border-bottom-color="black" border-bottom-style="solid" border-bottom-width="1px" border-collapse="collapse" border-left-color="black" border-left-style="solid" border-left-width="1px" margin-left="0" border-right-color="black" border-right-style="solid" border-right-width="1px" margin-right="0" border-top-color="black" border-top-style="solid" border-top-width="1px" id="unique_55_Connect_42_table_gqd_mgp_plb" chfox:nd-id="table_gqd_mgp_plb"><fo:table-column column-width="50%"/><fo:table-column column-width="50%"/><fo:table-header font-weight="bold" text-align="center">
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" id="unique_55__unique_55_Connect_42_table_gqd_mgp_plb__entry__1"><fo:block>Option</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" id="unique_55__unique_55_Connect_42_table_gqd_mgp_plb__entry__2"><fo:block>Description</fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  </fo:table-header><fo:table-body>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block><fo:inline font-family="monospace" font-size="smaller" role="Code">-d
                                                  &lt;directory&gt;</fo:inline></fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Source directory
                                                  containing the custom rules attribute dictionary
                                                  files to distribute. This is not required when run
                                                  on an ME to distribute files that came from an
                                                  SM.</fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block><fo:inline font-family="monospace" font-size="smaller" role="Code">-e &lt;MEs&gt;</fo:inline></fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Excludes the MEs
                                                  (comma-separated OAME VIPs of MEs) from
                                                  distribution. Only meaningful when run on SM.</fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block><fo:inline font-family="monospace" font-size="smaller" role="Code">-i &lt;MEs&gt;</fo:inline></fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Includes the MEs
                                                  (comma-separated OAME VIPs of MEs) for
                                                  distribution. Only meaningful when run on SM.</fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block><fo:inline font-family="monospace" font-size="smaller" role="Code">-h</fo:inline></fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Displays help for this
                                                  tool.</fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  </fo:table-body></fo:table></fo:block-container>The command performs the following:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_55_Connect_42_ul_hby_rgp_plb" chfox:nd-id="ul_hby_rgp_plb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Pushes the dictionary files
                                                  to the SM application nodes in that SM site.</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Reloads rules in all the SM
                                                  app nodes.</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Creates an archive of the
                                                  dictionary files and pushes it to the Active OAME
                                                  node on the ME sites.</fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">5.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">For the
                                                  PCRFCustomRuleAttributeDictionary.xml file,
                                                  ChargingCustomRuleAttributeDictionary.xml,
                                                  diameter/rule/ ChargingVsaToContextMapping.xml,
                                                  diameter/rule/VsaToContextMapping.xml or
                                                  RSCustomRuleAttributeDictionary.xml run the
                                                  following command on each ME's Active
                                                  OAME:<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>/opt/tpa/bin/distribute-rules-dictionaries</fo:inline></fo:block></fo:block-container>The
                                                  command performs the following:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_55_Connect_42_ul_lnd_vgp_plb" chfox:nd-id="ul_lnd_vgp_plb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Extracts the archive already
                                                  transferred from the SM.</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Pushes the dictionary files
                                                  to the Diameter applications and Common Services
                                                  nodes in the ME site.</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Reloads the rules in all the
                                                  applicable nodes.</fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                            </fo:list-block></fo:block>
                                    </fo:block></fo:block-container>
                                    <fo:block-container space-after="1em" space-before="1em" id="unique_55_Connect_42_section_int_wgp_plb" chfox:nd-id="section_int_wgp_plb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                        <fo:block-container id="d34e41440" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H3">To
                                            add to the attribute definition and custom vendor
                                            definition files</fo:block></fo:block-container>
                                        <fo:block>Use this
                                            procedure to add VSAs and vendor definitions to the
                                            attribute definition, and custom vendor definition
                                            files. Both the AttributeDefinition.xml and
                                            CustomVendorDefinition.xml exist in
                                            /opt/tpa/conf/sac/diameter/vsa on all SM, ME IOHD,
                                            Comservice, and Diameter nodes. Other definition files
                                            can also exist in the directory as you require them for
                                            your configuration and can be distributed in the same
                                                manner.<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="1.33em" space-before="1.33em" id="unique_55_Connect_42_ol_vhp_xgp_plb" chfox:nd-id="ol_vhp_xgp_plb">
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">1.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Connect to the SM active
                                                  OAME as the root user.</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">2.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">Open the
                                                  AttributeDefinition.xml and
                                                  CustomVendorDefinition.xml files that you need
                                                  from the /opt/tpa/conf/sac/diameter/vsa
                                                  directory using a text editor. The following files
                                                  are sample files that are installed by default.<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_55_Connect_42_ul_iwx_ygp_plb" chfox:nd-id="ul_iwx_ygp_plb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>/opt/tpa/conf/sac/diameter/vsa/AttributeDefinitionTemplate.xml</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>/opt/tpa/conf/sac/diameter/vsa/CustomVendorDefinition.xml</fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">3.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">To add or modify a
                                                  dictionary file, define the vendor_name and
                                                  vendor_id in the CustomVendorDefinition.xml file
                                                  to notify the system that the vendor is valid and
                                                  supported by the SPS. For example, to add
                                                  Motorola, type:
                                                  <fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>&lt;vendor id=161 name=MOT/&gt;</fo:inline></fo:block></fo:block-container></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">4.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Save and close the
                                                  CustomVendorDefinition.xml file.</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">5.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">Open or create a vendor
                                                  dictionary file to add or modify attributes. Use
                                                  the following format for the filename:
                                                  AttributeDefinition.xml; for
                                                  example,<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>AttributeDefinitionTemplate.xml</fo:inline></fo:block></fo:block-container>You
                                                  can modify all of the parameters in the .xml file,
                                                  which are described in the following table. The
                                                  AttributeDefinition&lt;VENDOR_NAME&gt;.xml file
                                                  defines a list of VSAs. Each vendor has a separate
                                                  file.<fo:block-container><fo:block font-weight="bold" space-before="0.5em" page-break-after="avoid" text-align="left"/><fo:table relative-align="baseline" chfox:html-table-width-policy="yes" table-layout="auto" border-bottom-color="black" border-bottom-style="solid" border-bottom-width="1px" border-collapse="collapse" border-left-color="black" border-left-style="solid" border-left-width="1px" margin-left="0" border-right-color="black" border-right-style="solid" border-right-width="1px" margin-right="0" border-top-color="black" border-top-style="solid" border-top-width="1px" id="unique_55_Connect_42_table_a41_fhp_plb" chfox:nd-id="table_a41_fhp_plb"><fo:table-column column-width="50%"/><fo:table-column column-width="50%"/><fo:table-header font-weight="bold" text-align="center">
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" id="unique_55__unique_55_Connect_42_table_a41_fhp_plb__entry__1"><fo:block>Parameter</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" id="unique_55__unique_55_Connect_42_table_a41_fhp_plb__entry__2"><fo:block>Description</fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  </fo:table-header><fo:table-body>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>attribute code</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Integer assigned to the
                                                  attribute by the vendor</fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>&lt;vendorName&gt;</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Each vendor has a unique
                                                  vendor ID.</fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>&lt;name&gt;</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Name of the
                                                  attribute</fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>&lt;format&gt;</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>The Diameter AVP
                                                  format</fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>&lt;mFlag&gt;</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Determines whether AVP
                                                  support is mandatory. The options are:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_55_Connect_42_ul_epv_3hp_plb" chfox:nd-id="ul_epv_3hp_plb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>REQUIRED*</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>OPTIONAL</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>FORBIDDEN</fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block>Must be set to REQUIRED; otherwise, the VSA
                                                  is not loaded. </fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>&lt;pFlag&gt;</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Determines whether
                                                  end-to-end encryption is required. The options
                                                  are:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_55_Connect_42_ul_tyr_khp_plb" chfox:nd-id="ul_tyr_khp_plb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>REQUIRED</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>OPTIONAL</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>FORBIDDEN</fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block></fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>&lt;vFlag&gt;</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Any attribute that is a
                                                  VSA and is added to a vendor dictionary must
                                                  specify REQUIRED for the in its definition;
                                                  otherwise, the VSA is not loaded.<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_55_Connect_42_ul_q12_mhp_plb" chfox:nd-id="ul_q12_mhp_plb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>REQUIRED</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>OPTIONAL</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>FORBIDDEN</fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block>This restriction does not apply to standard
                                                  attributes.</fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>&lt;encrypt&gt;</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>The type of encryption
                                                  used to encrypt and decrypt the attribute. The
                                                  options are:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_55_Connect_42_ul_t2l_nhp_plb" chfox:nd-id="ul_t2l_nhp_plb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>True to encrypt</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>False otherwise</fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block></fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>&lt;register&gt;</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Determines whether the
                                                  Diameter AVP definition should be registered. The
                                                  options are:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_55_Connect_42_ul_uvd_php_plb" chfox:nd-id="ul_uvd_php_plb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>True (to register)</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>False otherwise</fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block></fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>&lt;proprietary&gt;</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Determines whether the
                                                  dictionary is proprietary. The options are:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_55_Connect_42_ul_mkn_qhp_plb" chfox:nd-id="ul_mkn_qhp_plb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>True (VSA, proprietary)</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>False (standard
                                                  non-proprietary)</fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block></fo:block></fo:table-cell>
                                                  </fo:table-row>
                                                  </fo:table-body></fo:table></fo:block-container></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">6.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Perform To install, copy,
                                                  and distribute the attribute definition and custom
                                                  vendor definition files to copy and distribute
                                                  the VsaToContextMapping.xml, attribute definition,
                                                  and custom vendor definition files.</fo:block></fo:list-item-body></fo:list-item>
                                            </fo:list-block></fo:block>
                                    </fo:block></fo:block-container>
                                    <fo:block-container space-after="1em" space-before="1em" id="unique_55_Connect_42_section_xdn_shp_plb" chfox:nd-id="section_xdn_shp_plb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                        <fo:block-container id="d34e41663" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H3">To
                                            install, copy, and distribute the attribute definition
                                            and custom vendor definition files</fo:block></fo:block-container>
                                        <fo:block>Use this CLI
                                            tool in the OAME to install, copy, and distribute the
                                            attribute definition, and custom vendor definition
                                            files, along with the VSA mapping file from the OAME to
                                            the SMs and MEs. Both the AttributeDefinition.xml and
                                            CustomVendorDefinition.xml exist in
                                            /opt/tpa/conf/sac/diameter/vsa on all ME IOHD,
                                            Comservice, and Diameter nodes. Other definition files
                                            can also exist in the directory as you require them for
                                            your configuration and can be distributed in the same
                                                manner.<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="1.33em" space-before="1.33em" id="unique_55_Connect_42_ol_uk3_thp_plb" chfox:nd-id="ol_uk3_thp_plb">
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">1.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>If this is an installation,
                                                  obtain the VsaToContextMapping.xml,
                                                  AttributeDefinition.xml and
                                                  CustomVendorDefinition.xml files that you need
                                                  from the appropriate repository location provided
                                                  by Nokia.</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">2.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Connect to the SM active
                                                  OAME as the root user.</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">3.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">Put the files obtained in
                                                  step 1 in a directory of your choice on the SMs
                                                  Active OAME. If you have added to the
                                                  VsaToContextMapping.xml, AttributeDefinition.xml
                                                  and CustomVendorDefinition.xml files that already
                                                  exist in the /opt/tpa/conf/sac/diameter/vsa
                                                  directory, go to the next step.<fo:block-container space-after="1em" space-before="1em"><fo:block end-indent="0" start-indent="0" role="Note"><fo:inline background-image="url('file:/C:/Program%20Files/Oxygen%20XML%20Author%2021/frameworks/dita/DITA-OT3.x/plugins/com.oxygenxml.webhelp.responsive/oxygen-webhelp/app/img/note.svg')" background-position-horizontal="left" background-position-vertical="center" background-repeat="no-repeat" font-weight="bold" padding-bottom="4px" padding-left="23px" padding-right="4px" padding-top="4px">Note:</fo:inline> Before
                                                  copying any files to any SPS VMs, the user must
                                                  execute the df and du Linux commands from each VM
                                                  to determine the storage available and whether or
                                                  not the copying of new files can proceed. Usage of
                                                  the Linux commands df (abbreviation for disk free)
                                                  and du (abbreviated from disk usage) can be found
                                                  on every VM via the Linux manual page by using
                                                  man df and man du as root user. There are two
                                                  storage directories that can be used on any VM as
                                                  follows: <fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_55_Connect_42_ul_a34_5hp_plb" chfox:nd-id="ul_a34_5hp_plb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>First choice, is under the
                                                  /appdata directory which is created and mounted to
                                                  the cinder volume (OpenStack)/independent disk
                                                  (VMware) on every VM.</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">Second choice, is under the
                                                  /var/log directory which is created and mounted to
                                                  the cinder volume (OpenStack)/independent disk
                                                  (VMware) on every VM. If there is enough storage
                                                  available, proceed. When you have completed the
                                                  task, clean-up the files that are no longer
                                                  needed. If there is not enough space, either do
                                                  some clean-up or contact Nokia
                                                  Support.<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>/opt/tpa/bin/distribute-diameter-dictionaries -d &lt;directory&gt;</fo:inline></fo:block></fo:block-container>Where
                                                  the is the directory containing the
                                                  VsaToContextMapping.xml, AttributeDefinition.xml
                                                  and CustomVendorDefinition.xml files. The command
                                                  performs the following:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_55_Connect_42_ul_zjr_whp_plb" chfox:nd-id="ul_zjr_whp_plb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Pushes the dictionary files to the SM application
                                                  nodes in that SM site.</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Reloads rules in all the SM app nodes.</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Creates an archive of the dictionary files and
                                                  pushes it to the Active OAME node on the ME
                                                  sites.</fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block></fo:block></fo:block-container></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">4.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">Run the following command on
                                                  each ME's Active
                                                  OAME:<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>/opt/tpa/bin/distribute-diameter-dictionaries</fo:inline></fo:block></fo:block-container>The
                                                  command performs the following:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_55_Connect_42_ul_gdw_zhp_plb" chfox:nd-id="ul_gdw_zhp_plb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Extracts the archive already
                                                  transferred from the SM.</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Pushes the dictionary files
                                                  to the Diameter applications and Common Services
                                                  nodes in the ME site.</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Reloads the rules in all the
                                                  applicable nodes.</fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                            </fo:list-block></fo:block>
                                    </fo:block></fo:block-container>
                                </fo:block></fo:block-container>
                            </fo:block>
                        </fo:block></fo:block-container></fo:block></fo:block></fo:block></fo:block></fo:block></fo:block-container></fo:block></fo:flow></fo:page-sequence><fo:page-sequence master-reference="chapter" force-page-count="no-force" line-height-shift-adjustment="disregard-shifts"><fo:static-content flow-name="sc-before-left" role="artifact"><fo:table border-collapse="separate" table-layout="fixed" width="100%"><fo:table-body><fo:table-row><!--TOP-LEFT--><fo:table-cell start-indent="0" end-indent="0" height="1in" width="(8.5in - 1in - 1in)" text-align="left" display-align="center" font-size="8pt"><fo:block><fo:retrieve-marker retrieve-class-name="maptitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/><fo:retrieve-marker retrieve-class-name="parttitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/><fo:retrieve-marker retrieve-class-name="chaptertitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/><fo:retrieve-marker retrieve-class-name="sectiontitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/> | <fo:page-number/></fo:block></fo:table-cell></fo:table-row></fo:table-body></fo:table></fo:static-content><fo:static-content flow-name="sc-before-right" role="artifact"><fo:table border-collapse="separate" table-layout="fixed" width="100%"><fo:table-body><fo:table-row><!--TOP-RIGHT--><fo:table-cell start-indent="0" end-indent="0" height="1in" width="(8.5in - 1in - 1in)" text-align="right" display-align="center" font-size="8pt"><fo:block><fo:retrieve-marker retrieve-class-name="maptitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/><fo:retrieve-marker retrieve-class-name="parttitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/><fo:retrieve-marker retrieve-class-name="chaptertitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/><fo:retrieve-marker retrieve-class-name="sectiontitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/> | <fo:page-number/></fo:block></fo:table-cell></fo:table-row></fo:table-body></fo:table></fo:static-content><fo:static-content flow-name="xsl-footnote-separator" role="artifact" text-align="left" display-align="auto" font-size="8pt" border-top-style="solid" border-top-color="black" border-top-width="thin" padding-top="8pt"><fo:block/></fo:static-content><fo:flow flow-name="xsl-region-body"><fo:block font-family="serif, Times, Times New Roman, Symbol, MingLiU, MS Mincho, Arial Unicode MS" font-size="12px"><fo:block-container font-size="11.5pt" margin-left="0" margin-right="0"><fo:block end-indent="0" start-indent="0"><fo:block><fo:block><fo:block><fo:block xml:lang="en"><fo:block-container space-before="1em" id="unique_56" chfox:nd-id="troubleshooting"><fo:block end-indent="0" start-indent="0" role="Sect">
                            <fo:block-container font-size="2em" space-after="1em" margin-left="0" margin-right="0" space-before="0" page-break-after="avoid" id="ariaid-title56"><fo:block end-indent="0" start-indent="0" role="H1"><fo:marker marker-class-name="chaptertitle"><fo:inline/> | 10 - <!--content(text)--><fo:inline>Troubleshooting</fo:inline></fo:marker><fo:inline>Chapter 10. </fo:inline>Troubleshooting</fo:block></fo:block-container>
                            
                            <fo:block role="Sect" id="unique_57" chfox:nd-id="checking_logging_levels">
                                <fo:block-container font-size="1.7em" space-after="0.5em" margin-left="0" margin-right="0" space-before="1.5em" page-break-after="avoid" id="ariaid-title57"><fo:block end-indent="0" start-indent="0" role="H2">Changing
                                    the logging levels of SPS functions</fo:block></fo:block-container>
                                
                                <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                    <fo:block-container space-after="1em" space-before="1em" id="unique_57_Connect_42_section_un3_l5x_nlb" chfox:nd-id="section_un3_l5x_nlb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                        <fo:block-container id="d34e41836" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H3">Overview</fo:block></fo:block-container>
                                        <fo:block>This procedure
                                            is for changing the logging levels for the Java
                                            application and is not applicable to the database, CDR,
                                            or any /var/log/. Perform this procedure to modify the
                                            logging level of one or more SPS functions for
                                            troubleshooting purposes. See Changing SPS logging
                                            levelsfor information about the logging levels.<fo:block-container space-after="1em" space-before="1em"><fo:block end-indent="0" start-indent="0" role="Note"><fo:inline background-image="url('file:/C:/Program%20Files/Oxygen%20XML%20Author%2021/frameworks/dita/DITA-OT3.x/plugins/com.oxygenxml.webhelp.responsive/oxygen-webhelp/app/img/note.svg')" background-position-horizontal="left" background-position-vertical="center" background-repeat="no-repeat" font-weight="bold" padding-bottom="4px" padding-left="23px" padding-right="4px" padding-top="4px">Note:</fo:inline>
                                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Lowering a logging level creates additional log
                                                  entries and requires more system resources, which
                                                  can affect system performance. Ensure that you
                                                  lower the logging level for only the required
                                                  functions and for only the time required to
                                                  collect the troubleshooting information.</fo:block></fo:block-container>
                                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">For
                                                  day-to-day operations, be sure to review the
                                                  aggregated logs in the AdminCLI /var/log/
                                                  directory rather than the logs on each node in the
                                                  /opt/tpa/log/ directory.</fo:block></fo:block-container>
                                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Modifications to the /opt/tpa/logs/log4j.xml file
                                                  are not automatically carried over during an
                                                  upgrade. If you modify the log4j.xml file, after
                                                  an upgrade you must manually merge your changes
                                                  from the previous /opt//logs/log4j.xml file to the
                                                  new /opt/ /logs/log4j.xml file.</fo:block></fo:block-container>
                                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Application logs INFO level and lower are sent to
                                                  the OAM from the application nodes.</fo:block></fo:block-container>
                                            </fo:block></fo:block-container></fo:block>
                                    </fo:block></fo:block-container>
                                    <fo:block-container space-after="1em" space-before="1em" id="unique_57_Connect_42_section_efv_r5x_nlb" chfox:nd-id="section_efv_r5x_nlb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                        <fo:block-container id="d34e41863" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H3">Steps</fo:block></fo:block-container>
                                        <fo:block>
                                            <fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="1.33em" space-before="1.33em" id="unique_57_Connect_42_ol_adl_s5x_nlb" chfox:nd-id="ol_adl_s5x_nlb">
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">1.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Open a console window and
                                                  log in as tpaadmin to the specific node where you
                                                  want to modify the log level.</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">2.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Open the log4j.xml file in
                                                  /opt/tpa/logs/ directory using a text editor.</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">3.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">Perform any of the
                                                  following:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="0em" space-before="0em" id="unique_57_Connect_42_ul_ubx_55x_nlb" chfox:nd-id="ul_ubx_55x_nlb">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">To change the logging level
                                                  for general SPS functions, replace <fo:inline font-style="italic">logging_level</fo:inline> in the following with the
                                                  logging level that you need, for example,
                                                  DEBUG.<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>&lt;logger name="com.nokia.tpapps."&gt;
&lt;level value="logging_level"/&gt;
&lt;/logger&gt;</fo:inline></fo:block></fo:block-container></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">To change the logging level
                                                  for the Diameter protocol, replace <fo:inline font-style="italic">logging_level</fo:inline> in the following with the
                                                  logging level that you need, for example,
                                                  DEBUG.<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>&lt;logger name="com.nokia.tpapps.diameter"&gt;
&lt;level value="logging_level"/&gt;
&lt;/logger&gt;</fo:inline></fo:block></fo:block-container></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Restart the application
                                                  server after modifying log4j.xml.</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Save and close the log4j.xml
                                                  file.</fo:block></fo:list-item-body></fo:list-item>
                                                  </fo:list-block></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                            </fo:list-block>
                                        </fo:block>
                                    </fo:block></fo:block-container>
                                </fo:block></fo:block-container>
                            </fo:block>
                            <fo:block role="Sect" id="unique_58" chfox:nd-id="jaeger">
                                <fo:block-container font-size="1.7em" space-after="0.5em" margin-left="0" margin-right="0" space-before="1.5em" page-break-after="avoid" id="ariaid-title58"><fo:block end-indent="0" start-indent="0" role="H2">Jaeger</fo:block></fo:block-container>
                                
                                <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                    <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Jaeger is a
                                        distributed tracing platform which leverages OpenTracing to
                                        deliver analytics and visualizations regarding system
                                        performance. For more information, see the <fo:inline color="blue" text-decoration="underline"><fo:basic-link show-destination="new" indicate-destination="true" external-destination="https://www.jaegertracing.io/docs/1.17/">Jaeger documentation
                                            portal</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" external-destination="https://www.jaegertracing.io/docs/1.17/"><fo:inline font-style="italic" text-decoration="underline"/></fo:basic-link></fo:inline>.</fo:block></fo:block-container>
                                </fo:block></fo:block-container>
                                <fo:block role="Sect" id="unique_59" chfox:nd-id="accessing_jaeger">
                                    <fo:block-container font-size="1.4em" font-weight="300" space-after="0.4em" margin-left="0" margin-right="0" space-before="1.3em" page-break-after="avoid" id="ariaid-title59"><fo:block end-indent="0" start-indent="0" role="H3">Accessing
                                        the Jaeger console</fo:block></fo:block-container>
                                    
                                    <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                        <fo:block>To access the
                                            Jaeger console, navigate to the following link in a web
                                            browser:<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>https://&lt;sps_server_ip&gt;:&lt;jaeger_port&gt;</fo:inline></fo:block></fo:block-container></fo:block>
                                        <fo:block>Where:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="1.33em" space-before="1.33em" id="unique_59_Connect_42_ul_ij3_1bf_rlb" chfox:nd-id="ul_ij3_1bf_rlb">
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline font-family="monospace" font-size="smaller" role="Code">&lt;sps_server_ip&gt;</fo:inline> is the address for
                                                  the SPS server.</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline font-family="monospace" font-size="smaller" role="Code">&lt;jaeger_port&gt;</fo:inline>
                                                  is the port on which the Jaeger console is
                                                  running.</fo:block></fo:list-item-body></fo:list-item>
                                            </fo:list-block></fo:block>
                                    </fo:block></fo:block-container>
                                </fo:block>
                                <fo:block role="Sect" id="unique_60" chfox:nd-id="configuring_jaeger_yaml">
                                    <fo:block-container font-size="1.4em" font-weight="300" space-after="0.4em" margin-left="0" margin-right="0" space-before="1.3em" page-break-after="avoid" id="ariaid-title60"><fo:block end-indent="0" start-indent="0" role="H3">Jaeger
                                        configurations in the values.yaml</fo:block></fo:block-container>
                                    
                                    <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                        <fo:block-container space-after="1em" space-before="1em" id="unique_60_Connect_42_section_scg_hbf_rlb" chfox:nd-id="section_scg_hbf_rlb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                            <fo:block-container id="d34e42074" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H4">Overview</fo:block></fo:block-container>
                                            <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Jaeger
                                                configurations may be made in the values.yaml file
                                                as part of the OpenTracing configurations.</fo:block></fo:block-container>
                                            <fo:block>The
                                                following is an example of the OpenTracing and
                                                Jaeger configurations in the
                                                values.yaml.<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>opentracing:
    headers: "x-request-id,x-b3-traceid,x-b3-spanid,x-b3-parentspanid,x-b3-sampled,x-b3-flags,b3"
    jaeger:
      endpoint: "http://jaeger-collector.istio-system.svc.cluster.local:14268/api/traces"
</fo:inline></fo:block></fo:block-container></fo:block>
                                        </fo:block></fo:block-container>
                                        <fo:block-container space-after="1em" space-before="1em" id="unique_60_Connect_42_section_e3g_dbf_rlb" chfox:nd-id="section_e3g_dbf_rlb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                            <fo:block-container id="d34e42091" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H4">Header
                                                configuration</fo:block></fo:block-container>
                                            <fo:block>In the
                                                values.yaml, the <fo:inline font-family="monospace" font-size="smaller" role="Code">headers</fo:inline> property
                                                can be used to configure which headers are
                                                propagated.<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>opentracing:
    header: &lt;your_headers_here&gt;</fo:inline></fo:block></fo:block-container></fo:block>
                                            <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Where
                                                  <fo:inline font-family="monospace" font-size="smaller" role="Code">&lt;your_headers_here&gt;</fo:inline> is a comma
                                                separated list of headers to propagate.</fo:block></fo:block-container>
                                            <fo:block>For
                                                example, the following is configured to propagate
                                                headers <fo:inline font-family="monospace" font-size="smaller" role="Code">x-request-id</fo:inline>, <fo:inline font-family="monospace" font-size="smaller" role="Code">x-b3-traceid</fo:inline>, <fo:inline font-family="monospace" font-size="smaller" role="Code">x-b3-spanid</fo:inline>, <fo:inline font-family="monospace" font-size="smaller" role="Code">x-b3-parentspanid</fo:inline>,
                                                  <fo:inline font-family="monospace" font-size="smaller" role="Code">x-b3-sampled</fo:inline>, <fo:inline font-family="monospace" font-size="smaller" role="Code">x-b3-flags</fo:inline>, and
                                                  <fo:inline font-family="monospace" font-size="smaller" role="Code">b3</fo:inline>.<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>opentracing:
    headers: "x-request-id,x-b3-traceid,x-b3-spanid,x-b3-parentspanid,x-b3-sampled,x-b3-flags,b3"</fo:inline></fo:block></fo:block-container></fo:block>
                                        </fo:block></fo:block-container>
                                        <fo:block-container space-after="1em" space-before="1em" id="unique_60_Connect_42_section_t43_gbf_rlb" chfox:nd-id="section_t43_gbf_rlb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                            <fo:block-container id="d34e42142" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H4">Endpoint
                                                configuration</fo:block></fo:block-container>
                                            <fo:block>In the
                                                values.yaml, the <fo:inline font-family="monospace" font-size="smaller" role="Code">endpoint</fo:inline> property
                                                can be used to configure endpoints, which tell
                                                Jaeger where to send traces collected in
                                                SPS.<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>opentracing:
    headers: "x-request-id,x-b3-traceid,x-b3-spanid,x-b3-parentspanid,x-b3-sampled,x-b3-flags,b3"
    jaeger:
      endpoint: &lt;endpoint_address&gt;</fo:inline></fo:block></fo:block-container></fo:block>
                                            <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Where
                                                  <fo:inline font-family="monospace" font-size="smaller" role="Code">&lt;endpoint_address&gt;</fo:inline> is the address of
                                                the endpoint to which Jaeger will send traces.</fo:block></fo:block-container>
                                            <fo:block>For
                                                example, the following is configuration sends traces
                                                to the endpoint at <fo:inline font-family="monospace" font-size="smaller" role="Code">http://jaeger-collector.istio-system.svc.cluster.local:14268/api/traces</fo:inline>.<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>opentracing:
    headers: "x-request-id,x-b3-traceid,x-b3-spanid,x-b3-parentspanid,x-b3-sampled,x-b3-flags,b3"
    jaeger:
      endpoint: "http://jaeger-collector.istio-system.svc.cluster.local:14268/api/traces"
</fo:inline></fo:block></fo:block-container></fo:block>
                                        </fo:block></fo:block-container>
                                    </fo:block></fo:block-container>
                                </fo:block>
                            </fo:block>
                        </fo:block></fo:block-container></fo:block></fo:block></fo:block></fo:block></fo:block></fo:block-container></fo:block></fo:flow></fo:page-sequence><fo:page-sequence master-reference="chapter" force-page-count="no-force" id="last-page-sequence" line-height-shift-adjustment="disregard-shifts"><fo:static-content flow-name="sc-before-left" role="artifact"><fo:table border-collapse="separate" table-layout="fixed" width="100%"><fo:table-body><fo:table-row><!--TOP-LEFT--><fo:table-cell start-indent="0" end-indent="0" height="1in" width="(8.5in - 1in - 1in)" text-align="left" display-align="center" font-size="8pt"><fo:block><fo:retrieve-marker retrieve-class-name="maptitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/><fo:retrieve-marker retrieve-class-name="parttitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/><fo:retrieve-marker retrieve-class-name="chaptertitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/><fo:retrieve-marker retrieve-class-name="sectiontitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/> | <fo:page-number/></fo:block></fo:table-cell></fo:table-row></fo:table-body></fo:table></fo:static-content><fo:static-content flow-name="sc-before-right" role="artifact"><fo:table border-collapse="separate" table-layout="fixed" width="100%"><fo:table-body><fo:table-row><!--TOP-RIGHT--><fo:table-cell start-indent="0" end-indent="0" height="1in" width="(8.5in - 1in - 1in)" text-align="right" display-align="center" font-size="8pt"><fo:block><fo:retrieve-marker retrieve-class-name="maptitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/><fo:retrieve-marker retrieve-class-name="parttitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/><fo:retrieve-marker retrieve-class-name="chaptertitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/><fo:retrieve-marker retrieve-class-name="sectiontitle" retrieve-boundary="document" retrieve-position="first-including-carryover"/> | <fo:page-number/></fo:block></fo:table-cell></fo:table-row></fo:table-body></fo:table></fo:static-content><fo:static-content flow-name="xsl-footnote-separator" role="artifact" text-align="left" display-align="auto" font-size="8pt" border-top-style="solid" border-top-color="black" border-top-width="thin" padding-top="8pt"><fo:block/></fo:static-content><fo:flow flow-name="xsl-region-body"><fo:block font-family="serif, Times, Times New Roman, Symbol, MingLiU, MS Mincho, Arial Unicode MS" font-size="12px"><fo:block-container font-size="11.5pt" margin-left="0" margin-right="0"><fo:block end-indent="0" start-indent="0"><fo:block><fo:block><fo:block><fo:block xml:lang="en"><fo:block-container space-before="1em" id="unique_61" chfox:nd-id="sps_tools"><fo:block end-indent="0" start-indent="0" role="Sect">
                            <fo:block-container font-size="2em" space-after="1em" margin-left="0" margin-right="0" space-before="0" page-break-after="avoid" id="ariaid-title61"><fo:block end-indent="0" start-indent="0" role="H1"><fo:marker marker-class-name="chaptertitle"><fo:inline/> | 11 - <!--content(text)--><fo:inline>SPS tools</fo:inline></fo:marker><fo:inline>Chapter 11. </fo:inline>SPS
                                tools</fo:block></fo:block-container>
                            
                            <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">This appendix describes the tools that are available outside of
                                    the Service Manager GUI. Although most provisioning and
                                    configuration is done using the Service Manager GUI, the SPS
                                    also features tools for other specific tasks.</fo:block></fo:block-container>
                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">SPS database is managed using a series of command line tools.
                                    The tools may be run by the customer or by the Nokia support
                                    personnel. The tools are provided on the AdminCLI pod, a
                                    singleton pod specifically for this purpose. A kubectl command
                                    will be run by users to gain access to the AdminCLI pod for
                                    running these SPS tools and scripts.</fo:block></fo:block-container>
                                <fo:block-container space-after="1em" space-before="1em" id="unique_61_Connect_42_section_ixr_sz3_nlb" chfox:nd-id="section_ixr_sz3_nlb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                    <fo:block-container id="d34e42246" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H2">Supported
                                        tools</fo:block></fo:block-container>
                                    <fo:block>The following tools
                                        are supported:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="1.33em" space-before="1.33em" id="unique_61_Connect_42_ul_zlw_xz3_nlb" chfox:nd-id="ul_zlw_xz3_nlb">
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline font-style="italic">configure-manual-oos</fo:inline>: manually declares a
                                                component that is in a geo-redundant deployment out
                                                of service.</fo:block></fo:list-item-body></fo:list-item>
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline font-style="italic">db-backup</fo:inline>: creates a backup of the Aerospike
                                                database</fo:block></fo:list-item-body></fo:list-item>
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline font-style="italic">db-list</fo:inline>: lists the records in the Aerospike
                                                database, which is written in a compressed (.gz)
                                                files in a JSON format.</fo:block></fo:list-item-body></fo:list-item>
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline font-style="italic">db-stats</fo:inline>: displays Aerospike database
                                                statistics</fo:block></fo:list-item-body></fo:list-item>
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline font-style="italic">log-level</fo:inline>: provides get/set/reset log levels
                                                from the AdminCLI pod to all application servers in
                                                the cluster.</fo:block></fo:list-item-body></fo:list-item>
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline font-style="italic">network-heartbeat</fo:inline>: retrieves the heartbeat
                                                status on demand.</fo:block></fo:list-item-body></fo:list-item>
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline font-style="italic">sps-status</fo:inline>: displays status information for
                                                this SPS system.</fo:block></fo:list-item-body></fo:list-item>
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline font-style="italic">xdr-status</fo:inline>: </fo:block></fo:list-item-body></fo:list-item>
                                        </fo:list-block></fo:block>
                                </fo:block></fo:block-container>
                            </fo:block></fo:block-container>
                            <fo:block role="Sect" id="unique_62" chfox:nd-id="configure-manual-oos">
                                <fo:block-container font-size="1.7em" space-after="0.5em" margin-left="0" margin-right="0" space-before="1.5em" page-break-after="avoid" id="ariaid-title62"><fo:block end-indent="0" start-indent="0" role="H2">configure-manual-oos</fo:block></fo:block-container>
                                
                                <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                    <fo:block-container space-after="1em" space-before="1em" id="unique_62_Connect_42_section_qp5_fns_nlb" chfox:nd-id="section_qp5_fns_nlb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                        <fo:block-container id="d34e42344" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H3">Description</fo:block></fo:block-container>
                                        <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">This command
                                            is run as the root user or tpaadmin from /opt/tpa/bin
                                            and used to manually declare a component that is in a
                                            geo-redundant deployment out of service. It can set and
                                            clear the manual out of service flag for a component
                                            (stored in SDC).</fo:block></fo:block-container>
                                        <fo:block>When a site is
                                            set OOS using configure-manual-oos:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="1.33em" space-before="1.33em" id="unique_62_Connect_42_ul_b15_34s_nlb" chfox:nd-id="ul_b15_34s_nlb">
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>other sites in the
                                                  geo-redundant configuration see this site as
                                                  Out-Of-Service (SMs and MEs), until it is returned
                                                  to service using configure-manual-oos -e
                                                  &lt;component name&gt;</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>there is a system wide
                                                  graceful shutdown of all Diameter links (Client or
                                                  SPS Initiated)</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>provisioning operations
                                                  cannot be executed on the OOS site (SM or ME),
                                                  until it is returned to service</fo:block></fo:list-item-body></fo:list-item>
                                            </fo:list-block></fo:block>
                                    </fo:block></fo:block-container>
                                    <fo:block-container space-after="1em" space-before="1em" id="unique_62_Connect_42_section_gym_k4s_nlb" chfox:nd-id="section_gym_k4s_nlb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                        <fo:block-container id="d34e42369" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H3">Syntax</fo:block></fo:block-container>
                                        <fo:block><fo:inline font-family="monospace" font-size="smaller" role="Code">configure-manual-oos [-d] [-e] [-h] [-s]</fo:inline><fo:block-container space-after="1em" space-before="1em"><fo:block end-indent="0" start-indent="0" role="Note"><fo:inline background-image="url('file:/C:/Program%20Files/Oxygen%20XML%20Author%2021/frameworks/dita/DITA-OT3.x/plugins/com.oxygenxml.webhelp.responsive/oxygen-webhelp/app/img/note.svg')" background-position-horizontal="left" background-position-vertical="center" background-repeat="no-repeat" font-weight="bold" padding-bottom="4px" padding-left="23px" padding-right="4px" padding-top="4px">Note:</fo:inline> The
                                                component name is case sensitive and must be entered
                                                as upper case ME or SM. For
                                                example:<fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code"><fo:inline>configure-manual-oos -d ME
</fo:inline></fo:block></fo:block-container></fo:block></fo:block-container></fo:block>
                                        <fo:block-container><fo:block font-weight="bold" space-before="0.5em" page-break-after="avoid" text-align="left"/><fo:table relative-align="baseline" chfox:html-table-width-policy="yes" table-layout="auto" border-bottom-color="black" border-bottom-style="solid" border-bottom-width="1px" border-collapse="collapse" border-left-color="black" border-left-style="solid" border-left-width="1px" margin-left="0" border-right-color="black" border-right-style="solid" border-right-width="1px" margin-right="0" border-top-color="black" border-top-style="solid" border-top-width="1px" id="unique_62_Connect_42_table_i2w_m4s_nlb" chfox:nd-id="table_i2w_m4s_nlb"><fo:table-column column-width="50%"/><fo:table-column column-width="50%"/><fo:table-header font-weight="bold" text-align="center">
                                                <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" id="unique_62__unique_62_Connect_42_table_i2w_m4s_nlb__entry__1"><fo:block>Code</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" id="unique_62__unique_62_Connect_42_table_i2w_m4s_nlb__entry__2"><fo:block>Description</fo:block></fo:table-cell>
                                                </fo:table-row>
                                            </fo:table-header><fo:table-body>
                                                <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block><fo:inline font-family="monospace" font-size="smaller" role="Code">-d &lt;component
                                                  name&gt;</fo:inline></fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Declares the component
                                                  manually out of service.</fo:block></fo:table-cell>
                                                </fo:table-row>
                                                <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block><fo:inline font-family="monospace" font-size="smaller" role="Code">-e &lt;component
                                                  name&gt;</fo:inline></fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Clear the component from
                                                  being declared manually out of service.</fo:block></fo:table-cell>
                                                </fo:table-row>
                                                <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block><fo:inline font-family="monospace" font-size="smaller" role="Code">-h or -?</fo:inline></fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Displays usage/help
                                                  information.</fo:block></fo:table-cell>
                                                </fo:table-row>
                                                <fo:table-row display-align="center">
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block><fo:inline font-family="monospace" font-size="smaller" role="Code">-s</fo:inline></fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" display-align="before"><fo:block>Print each component name
                                                  and whether or not it is manually out of service.
                                                  </fo:block></fo:table-cell>
                                                </fo:table-row>
                                            </fo:table-body></fo:table></fo:block-container>
                                        <fo:block-container space-after="1em" space-before="1em"><fo:block end-indent="0" start-indent="0" role="Note"><fo:inline background-image="url('file:/C:/Program%20Files/Oxygen%20XML%20Author%2021/frameworks/dita/DITA-OT3.x/plugins/com.oxygenxml.webhelp.responsive/oxygen-webhelp/app/img/note.svg')" background-position-horizontal="left" background-position-vertical="center" background-repeat="no-repeat" font-weight="bold" padding-bottom="4px" padding-left="23px" padding-right="4px" padding-top="4px">Note:</fo:inline> When
                                            this command is run on a non-Geo SPS, it will do nothing
                                            but Component ME State: UP will be displayed when
                                            running the sps-status tool.</fo:block></fo:block-container>
                                        <fo:block-container space-after="1em" space-before="1em"><fo:block end-indent="0" start-indent="0" role="Note"><fo:inline background-image="url('file:/C:/Program%20Files/Oxygen%20XML%20Author%2021/frameworks/dita/DITA-OT3.x/plugins/com.oxygenxml.webhelp.responsive/oxygen-webhelp/app/img/note.svg')" background-position-horizontal="left" background-position-vertical="center" background-repeat="no-repeat" font-weight="bold" padding-bottom="4px" padding-left="23px" padding-right="4px" padding-top="4px">Note:</fo:inline> This
                                            command is supported on an IG but requires that you
                                            state SM or ME to render the respective functionality on
                                            the IG OOS.</fo:block></fo:block-container>
                                    </fo:block></fo:block-container>
