                                <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                    <fo:block-container space-after="1em" space-before="1em" id="unique_14_Connect_42_section_zjc_cjt_mlb" chfox:nd-id="section_zjc_cjt_mlb"><fo:block end-indent="0" start-indent="0" role="Sect">
                                        <fo:block-container id="d34e6679" font-size="1.17em" font-weight="bold" space-after="0" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H3">Overview</fo:block></fo:block-container>
                                        <fo:block>SPS collects
                                            measurements for the following areas:<fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="1.33em" space-before="1.33em" id="unique_14_Connect_42_ul_kg4_kjt_mlb" chfox:nd-id="ul_kg4_kjt_mlb">
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Diameter (Base) metrics</fo:block></fo:list-item-body></fo:list-item>
                                                <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Diameter supplemental
