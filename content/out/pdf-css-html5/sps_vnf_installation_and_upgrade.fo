                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">c.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">
                                                  <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Verify
                                                  this VNF using the procedure <fo:inline><fo:inline font-style="italic"><fo:basic-link show-destination="new" indicate-destination="true" external-destination="unique_20"> (on page </fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" external-destination="unique_20"><fo:page-number-citation ref-id="unique_20"/></fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" external-destination="unique_20">)</fo:basic-link></fo:inline></fo:inline>.</fo:block></fo:block-container>
