                                                  <fo:inline color="blue" text-decoration="underline"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_22">Enabling Web SSO on SM App
                                                  VMs</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_22"><fo:inline font-style="italic" text-decoration="underline"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_22"> (on page </fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_22"><fo:page-number-citation ref-id="unique_22"/></fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_22">)</fo:basic-link></fo:inline></fo:basic-link></fo:inline>
