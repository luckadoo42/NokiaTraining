<concept id ="d1e369403"><title>EDR tags</title><conbody>
<section>

<table colsep="1" frame="all" pgwide="1" rowsep="1" scale="80">
                <title>EDR tags for LDAP search</title>
                <tgroup cols="3">
                    <colspec colname="col1" colwidth="1*"/>
                    <colspec colname="col2" colwidth="1.4*"/>
                    <colspec colname="col3" colwidth="1.64*"/>
                    <thead>
